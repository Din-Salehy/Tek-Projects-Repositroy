<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="DemoSuite">
	<test name="TekSchoolLogin">
		<classes>
			<class name="testNG.TekSchool">
				<methods>
					<include name="enterLoginDetails" />
				</methods>
			</class>
		</classes>
	</test>
	<test name="TestFacebook">
		<classes>
			<class name="testNG.FirstTest">
				<methods>
				<include name = "testFaceBook"/>
				</methods>
			</class>
		</classes>
	</test>
	<test name="TestGoogle">
		<classes>
			<class name="testNG.FirstTest">
				<methods>
					<include name="testGoogle" />
					<exclude name = "signIn"/>
				</methods>
			</class>
		</classes>
	</test>
</suite> 